{
  "name": "amt-deltaker-flate",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=local vite --port 3000 --mode local-dev",
    "build": "tsc && vite build",
    "build:gh-pages": "NODE_ENV=development vite build --mode gh-pages",
    "build:pull-request": "NODE_ENV=development vite build --mode pull-request",
    "test": "vitest",
    "test:no-watch": "vitest run",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --config .prettierrc.cjs -c src/**/*",
    "format:fix": "prettier --config .prettierrc.cjs -w src/**/*"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.cjs --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@navikt/aksel-icons": "^5.17.2",
    "@navikt/ds-css": "^5.17.2",
    "@navikt/ds-react": "^5.17.2",
    "@navikt/ds-tailwind": "^5.17.2",
    "@types/react-router-dom": "^5.3.3",
    "dayjs": "^1.11.10",
    "mockjs": "^1.1.0",
    "msw": "^2.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-router-dom": "^6.21.3",
    "swr": "^2.2.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "rollup-plugin-import-map": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
