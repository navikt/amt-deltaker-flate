{
  "name": "amt-deltaker-flate",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=local vite --port 3000",
    "build": "tsc && vite build --base=/deltaker/",
    "build:gh-pages": "NODE_ENV=development vite build --base=/amt-deltaker-flate/",
    "test": "vitest",
    "test:no-watch": "vitest run",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.cjs --fix"
    ]
  },
  "dependencies": {
    "@navikt/ds-css": "^5.11.4",
    "@navikt/ds-react": "^5.11.4",
    "mockjs": "^1.1.0",
    "msw": "^2.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "rollup-plugin-import-map": "^3.0.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
