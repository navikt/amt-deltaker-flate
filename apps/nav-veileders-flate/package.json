{
  "name": "nav-veileders-flate",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "VITE_ENDPOINT_HANDLER=MOCK VITE_MOCK_SERVICE_RUNNER_PATH=/mockServiceWorker.js vite --port 3004",
    "build:demo": "VITE_ENDPOINT_HANDLER=MOCK vite build",
    "build:pull-request": "VITE_ENDPOINT_HANDLER=DEV VITE_PR_ENV=pull_request vite build",
    "build:dev": "VITE_FARO_URL=https://telemetry.ekstern.dev.nav.no/collect VITE_ENDPOINT_HANDLER=DEV vite build --config vite-micro-frontend.config.js",
    "build": "VITE_FARO_URL=https://telemetry.nav.no/collect VITE_ENDPOINT_HANDLER=PROD vite build --config vite-micro-frontend.config.js",
    "test": "vitest",
    "test:no-watch": "vitest run",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.7.0",
    "@hookform/resolvers": "^3.3.4",
    "@navikt/aksel-icons": "^6.7.0",
    "@navikt/ds-css": "^6.7.0",
    "@navikt/ds-react": "^6.7.0",
    "@navikt/ds-tailwind": "^6.7.0",
    "dayjs": "^1.11.11",
    "deltaker-flate-common": "workspace:*",
    "mockjs": "^1.1.0",
    "msw": "^2.2.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-router-dom": "^6.23.0",
    "swr": "^2.2.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@amt-deltaker-flate/eslint-config": "workspace:*",
    "@amt-deltaker-flate/tailwind-config": "workspace:*",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "cpy-cli": "^5.0.0",
    "jsdom": "^24.0.0",
    "rollup-plugin-import-map": "^3.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
