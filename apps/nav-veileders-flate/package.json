{
  "name": "nav-veileders-flate",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=local vite --port 3000 --mode local-dev",
    "build": "tsc && vite build",
    "build:gh-pages": "NODE_ENV=development vite build --mode gh-pages",
    "build:pull-request": "NODE_ENV=development vite build --mode pull-request",
    "test": "vitest",
    "test:no-watch": "vitest run",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "preview": "vite preview",
    "format": "prettier --config .prettierrc.cjs -c src/**/*",
    "format:fix": "prettier --config .prettierrc.cjs -w src/**/*",
    "format:staged": "git diff --name-only --cached | xargs prettier --config .prettierrc.cjs -w"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@navikt/aksel-icons": "^6.1.0",
    "@navikt/ds-css": "^6.1.0",
    "@navikt/ds-react": "^6.1.0",
    "@navikt/ds-tailwind": "^6.1.0",
    "@types/react-router-dom": "^5.3.3",
    "dayjs": "^1.11.10",
    "mockjs": "^1.1.0",
    "msw": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router-dom": "^6.22.3",
    "swr": "^2.2.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "@repo/eslint-config": "workspace:*",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "rollup-plugin-import-map": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
